{
  "name": "twicher",
  "version": "0.0.1",
  "description": "Simple quotes editor",
  "dependencies": {
    "bower": "~1.4.1",
    "gulp": "~3.9.0",
    "browserify": "~10.2.4",
    "coffee-script": "~1.9.3",
    "coffeeify": "~1.1.0",
    "del": "~1.2.0",
    "gulp-rename": "~1.2.2",
    "gulp-sourcemaps": "~1.5.2",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "gulp-uglify": "~1.2.0",
    "gulp-stylus": "~2.0.3",
    "debowerify": "~1.3.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Orhideous/twicher.git"
  },
  "author": "Andriy Kushnir (Orhideous)",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/Orhideous/twicher/issues"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "debowerify"
    ],
    "extensions": [
      ".js",
      ".coffee"
    ]
  },
  "stylus": {
    "includes": [
      "./bower_components/bootstrap-stylus"
    ]
  },
  "assets": {
    "scripts": [
      "./twicher/static/src/js/main.coffee"
    ],
    "stylesheets": [
      "./twicher/static/src/css/main.styl"
    ]
  },
  "dist": "./twicher/static/dist"
}
